            android:layout_marginTop="10dp"
            android:layout_marginStart="16dp"
            android:layout_below="@id/main_search_text">

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/naver_search_btn"
